# 
# Definitions from configSection "frontpanel" in XML input file
#
#     lvDCOMConfigure("lvfp", "frontpanel", "\protocol\lv_RSVNA.xml")
# 

###############
# VNA records #
###############

# Read LabVIEW control/indicator "RF State (T)" on "RSVNA.vi"
record(bi, "$(P)VNA:STATE")
{
	field(DESC, "LabVIEW 'RF State (T)'")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn(lvfp,0,0)RF_State__T_")
    field(SCAN, ".1 second")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    info(INTEREST, "HIGH")
    info(archive, "VAL")
}

# Write to LabVIEW control "RF State (T)" on "RSVNA.vi"
record(bo, "$(P)VNA:STATE:SP")
{
	field(DESC, "LabVIEW 'RF State (T)'")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn(lvfp,0,0)RF_State__T_")   
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    info(INTEREST, "HIGH")
    info(archive, "VAL")
}

alias("$(P)VNA:STATE:SP", "$(P)VNA:STATE:SP:RBV")


